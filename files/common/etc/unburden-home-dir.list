# Generic cache locations
m D .cache/chromium cache_chromium
m D .config/google-chrome/Safe*Browsing google_chrome_safebrowsing
m D .config/google-chrome/component_crx_cache google_chrome_crx_cache
m D .config/chromium/component_crx_cache chromium_crx_cache
m D .config/google-chrome/*/Service*Worker/CacheStorage google_chrome_serviceworker_cache-%1
m D .config/chromium/*/Service*Worker/CacheStorage chromium_serviceworker_cache-%1

m D .config/discord/0.* discord_updates_0.%1
m D .config/discord/Cache  discord_cache
m D .config/discord/Code*Cache discord_code_cache
m D .config/discord/GPUCache discord_gpucache
m D .config/Code/Cache vscode_cache
m D .config/VSCodium/Cache vscodium_cache

m D .cache/mozilla cache_mozilla
m D .cache/netbeans cache_netbeans
m D .cache/epiphany-browser cache_epiphanybrowser
m D .cache/tracker cache_tracker
m D .cache/MonoDevelop-3.0 cache_monodevelop_3.0
m D .cache/google-chrome cache_googlechrome
m D .netbeans/*/var/cache netbeans_%1_var_cache
m D .local/share/tracker local_share_tracker
m D .thumbnails thumbnails
m D .CLion2017.2/system CLion2017.2
m D .IdeaIC2018.1/system IdeaIC2018.1
m D .WebStorm2017.2/system WebStorm2017.2
m D .PyCharmCE2019.2/system PyCharmCE2019.2

m D .ccache ccache

# Virtual machines setup
m D VirtualBox*VMs VirtualBox_VMs
m D .config/VirtualBox configr_VirtualBox

# xsession errors
m f .xsession-errors.old xsession-errors.old
m f .xsession-errors xsession-errors

# maven repos
m D .m2/repository maven_repository

# Common browser caches
m f .config/google-chrome/*/Thumbnails google-chrome-thumbnails-%1
m f .config/google-chrome/*/Thumbnails-journal google-chrome-thumbnails-journal-%1
m f .config/chromium/*/Thumbnails chromium-thumbnails-%1
m f .config/chromium/*/Thumbnails-journal chromium-thumbnails-journal-%1
#m d .mozilla/default/*/Cache mozilla-default-cache-%1
#m d .mozilla/default/*/startupCache mozilla-default-startup-cache-%1
#m d .mozilla/firefox/*/Cache firefox-cache-%1
#m d .mozilla/firefox/*/startupCache firefox-startup-cache-%1
#m d .mozilla/firefox/*/Cache.Trash firefox-cache-trash-%1
#m d .conkeror.mozdev.org/conkeror/*/Cache conkeror-cache-%1
#m d .conkeror.mozdev.org/conkeror/*/startupCache conkeror-startup-cache-%1
#m d .conkeror.mozdev.org/conkeror/*/Cache.Trash conkeror-cache-trash-%1
#m d .kazehakase/mozilla/kazehakase/Cache kazehakase-cache
#m d .galeon/mozilla/galeon/Cache galeon-cache
#m d .gnome2/epiphany/mozilla/epiphany/Cache epiphany-cache
